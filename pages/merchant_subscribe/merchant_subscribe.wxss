/* pages/merchant_subscribe/merchant_subscribe.wxss */

.status_circle {
  width: 20rpx;
  height: 20rpx;
  background-color: rgba(67, 186, 135, 1);
  border-radius: 100%;
}

.status_circle_alpha {
  width: 20rpx;
  height: 20rpx;
  background-color: rgba(67, 186, 135, 0.5);
  border-radius: 100%;
}

.status_line {
  margin-top: 12rpx;
  height: 4rpx;
  width: 60rpx;
  background-color: rgba(67, 186, 135, 1);
}

.status_line_alpha_50 {
  margin-top: 12rpx;
  height: 4rpx;
width: 60rpx;
  background-color: rgba(67, 186, 135, 0.5);
}

.status_line_translate {
  margin-top: 12rpx;
  height: 4rpx;
  width: 60rpx;
  background-color: rgba(67, 186, 135, 0);
}

.status_title {
  margin-top: 18rpx;
}



.wrap {
  position: relative;
  margin: 0 auto;
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}

.date {
  width: 223rpx;
  height: 50rpx;
  border-radius: 25rpx;
  background: #F7F7F7;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  line-height: 50rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-evenly;
  align-items: center;
}

.middle {
  font-size: 50rpx;
  font-weight: 400;
  line-height: 22rpx;
}

.date-down {
  width: 14rpx;
  height: 8rpx;
}