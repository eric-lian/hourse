/* pages/tab/merchants/merchants.wxss */
page {
  height: 100%;
  display: flex;
  flex-direction: column;
}

#getUserProfileBtn {
  background-color: #60CF9C;
  color: #FFFFFF;
  font-size: 28rpx;
  margin-top: 103rpx;
  width: 630rpx;
  height: 98rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 80rpx;
}

.register_status_container {
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
}

.register_status_body {
  background-color: white;
  border-radius: 24rpx 24rpx 0rpx 0rpx;
  height: 100%;
  margin-top: -26rpx;
  z-index: 1;
  display: flex;
  flex-direction: column;
}

.common_btn {
  background-color: rgba(96, 207, 156, 1);
  color: white;
  font-weight: 600;
  font-size: 28rpx;
  border-radius: 80rpx;
  justify-content: center;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.register_status_title {
  font-size: 28rpx;
  font-weight: 600;
  color: rgba(4, 5, 7, 1);
  margin-top: 40rpx;
  margin-left: 60rpx;
}

.register_result_container {
  /* background-color: violet; */
  display: flex;
  flex-direction: column;
  align-items: center;
}

.register_result_icon {
  width: 240rpx;
  height: 240rpx;
  margin-top: 52rpx;
}

.register_result_desc {
  font-weight: 600;
  font-size: 32rpx;
  color: rgba(96, 207, 156, 1);
  margin-top: 52rpx;
}