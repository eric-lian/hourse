/* pages/look_service/look_service.wxss */
page {
  height: 100%;
}
text {
  /* 控制text分行 */
  word-break: break-all;
}

.person_item {
  margin: 20rpx;
  display: flex;
  flex-direction: row;
  border-color: #F3F5F7;
  border-width: 2rpx;
  border-style: solid;
  padding: 20rpx;
  border-radius: 16rpx;
}

#immediately_subscribe {
  font-size: 22rpx;
  background-color: rgba(21, 92, 205, 1);
  border-radius: 80rpx;
  color: white;
  font-weight: 400;
  padding: 4rpx 12rpx 4rpx 12rpx;  
}



#immediately_call {
  font-size: 22rpx;
  background-color: rgba(21, 92, 205, 1);
  border-radius: 80rpx;
  color: white;
  font-weight: 400;
  padding: 4rpx 12rpx 4rpx 12rpx;  
  
}