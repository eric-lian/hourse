/**app.wxss**/
page {
  height: 100%;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.min_line {
  height: 2rpx;
  background-color: rgb(247, 245, 243);
  width: 100%;
}


.mid_line {
  background-color: #F9FAFB;
  height: 10rpx;
}

.line_32 {
  background-color: #F9FAFB;
  height: 32rpx;
}


.varify_passed_tag {
  background-color: rgba(229, 84, 43, 0.3);
  font-weight: 400;
  font-size: 22rpx;
  border-radius: 80rpx;
  width: 110rpx;
  height: 32rpx;
  align-content: center;
  display: flex;
  align-content: center;
  justify-content: center;
  color: #EC6A38;
} 


.common_btn {
  /* 155CCD */
  background-color: rgba(21, 92, 205, 1);
  color: white;
  font-weight: 600;
  font-size: 28rpx;
  border-radius: 18rpx;
  justify-content: center;
  display: flex;
  align-items: center;
  flex-direction: column;
}